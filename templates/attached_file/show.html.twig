{% extends 'base.html.twig' %}

{% block title %}AttachedFile{% endblock %}

{% block body %}
    <h1>AttachedFile</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ attached_file.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ attached_file.name }}</td>
            </tr>
            <tr>
                <th>Type</th>
                <td>{{ attached_file.type }}</td>
            </tr>
            <tr>
                <th>Upload_date</th>
                <td>{{ attached_file.uploadDate ? attached_file.uploadDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Validated</th>
                <td>{{ attached_file.validated ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>ImageName</th>
                <td>{{ attached_file.imageName }}</td>
            </tr>
            <tr>
                <th>UpdatedAt</th>
                <td>{{ attached_file.updatedAt ? attached_file.updatedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('attached_file_index') }}">back to list</a>

    <a href="{{ path('attached_file_edit', {'id': attached_file.id}) }}">edit</a>

    {{ include('attached_file/_delete_form.html.twig') }}
{% endblock %}
